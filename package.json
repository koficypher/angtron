{
  "name": "angtron",
  "version": "0.0.0",
  "main": "pre-dist/process.js",
  "scripts": {
    "ng": "ng serve",
    "clean": "rimraf dist pre-dist package-win installers",
    "rebuild": "electron-rebuild -f -w yourmodule",
    "electron": "wait-on http://localhost:4200 && cross-env ELECTRON_IS_DEV=true electron .",
    "electron-tsc": "tsc --project tsconfig.electron.json && npm run electron",
    "build-win": "tsc --project tsconfig.electron.json && ng build --configuration production && cp -r pre-dist/* dist/",
    "prepare-build-run": "cp package.dev.json dist/package.json && npm install --prefix ./dist/ && npm start --prefix ./dist/",
    "prepare-build": "cp package.dev.json dist/package.json && npm install --prefix ./dist/ ",
    "prestart": "npm run clean",
    "prebuild": "npm run clean",
    "build": "npm run build-win && npm run prepare-build",
    "build-run": "npm run build-win && npm run prepare-build-run",
    "start": "concurrently -k \"ng serve\" \"npm run electron-tsc\"",
    "pack-win": "electron-packager ./dist/ --overwrite --platform=win32 --arch=x64 --out=package-win",
    "install-win": "node ./installer.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.15",
    "@angular/common": "~12.2.15",
    "@angular/compiler": "~12.2.15",
    "@angular/core": "~12.2.15",
    "@angular/forms": "~12.2.15",
    "@angular/platform-browser": "~12.2.15",
    "@angular/platform-browser-dynamic": "~12.2.15",
    "@angular/router": "~12.2.15",
    "cross-env": "^7.0.2",
    "ngx-electron": "^2.2.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.14",
    "@angular/cli": "~12.2.14",
    "@angular/compiler-cli": "~12.2.15",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.2.0",
    "electron": "^21.0.1",
    "electron-packager": "^16.0.0",
    "electron-rebuild": "^1.11.0",
    "electron-winstaller": "^4.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.10",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5",
    "wait-on": "^5.0.1"
  }
}
